---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">{% for sp in site.pages %}{% capture lastchar %}{{ sp.url | remove:'index.html' | lastchar }}{% endcapture %}{% if lastchar == '/' %}
<url>
   <loc>{{ site.url }}{{ sp.url | remove:'index.html' }}</loc>
   <lastmod>{{ site.time | date:'%Y-%m-%d' }}</lastmod>
   <changefreq>daily</changefreq>
   <priority>0.8</priority>
</url>{% endif %}{% endfor %}{% for post in site.posts %}
<url>
   <loc>{{ site.url }}{{ post.url }}</loc>
   <lastmod>{{ site.time | date:'%Y-%m-%d' }}</lastmod>
   <changefreq>daily</changefreq>
   <priority>0.6</priority>
</url>{% endfor %}
</urlset>
